    "contents": [
        [
            {
                "type": "lyric",
                "text": "Revy,"
            },
            {
                "type": "lyric",
                "text": "rævva,"
            },
            {
                "type": "lyric",
                "text": "rimord, og rivaler."
            },
            {
                "type": "lyric",
                "text": "Rumba,"
            },
            {
                "type": "lyric",
                "text": "rytme"
            },
            {
                "type": "lyric",
                "text": "rimer – på doa-doa-doa."
            },
            {
                "type": "lyric",
                "text": "Sing-sing,"
            },
            {
                "type": "lyric",
                "text": "soft-ice,"
            },
            {
                "type": "lyric",
                "text": "striptease og sigarer."
            },
            {
                "type": "lyric",
                "text": "Og så blir det moll,"
            },
            {
                "type": "lyric",
                "text": "og så blir det Tott,"
            },
            {
                "type": "lyric",
                "text": "og så blir det Knoll"
            },
            {
                "type": "lyric",
                "text": "og så går det trått."
            },
            {
                "type": "lyric",
                "text": "Hjem langt"
            },
            {
                "type": "lyric",
                "text": "mot nord,"
            },
            {
                "type": "lyric",
                "text": "hjemme hos min morbror."
            },
            {
                "type": "lyric",
                "text": "Men det"
            },
            {
                "type": "lyric",
                "text": "var da"
            },
            {
                "type": "lyric",
                "text": "voldsomt  -  til sentimentalitet!"
            },
            {
                "type": "lyric",
                "text": "Skuff-skuff"
            },
            {
                "type": "lyric",
                "text": "skuvadeguveduff"
            },
            {
                "type": "lyric",
                "text": "fall oss"
            },
            {
                "type": "lyric",
                "text": "om halsen i"
            },
            {
                "type": "lyric",
                "text": "aften,"
            },
            {
                "type": "lyric",
                "text": "når du er her på vår"
            },
            {
                "type": "lyric",
                "text": "revy."
            }
        ]
    ]
}
