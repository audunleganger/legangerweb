    "contents": [
        [
            {
                "type": "lyric",
                "text": "Det var en deilig aften, jeg var knapt 19 år."
            },
            {
                "type": "lyric",
                "text": "Hun hadde røde lepper og blondt og vakkert hår."
            },
            {
                "type": "lyric",
                "text": "Pygminen var så deilig,"
            },
            {
                "type": "lyric",
                "text": "og jeg var mer enn klar."
            },
            {
                "type": "lyric",
                "text": "Ja, da visste jeg, ja, da visste jeg"
            },
            {
                "type": "lyric",
                "text": "hvorhen, hvorhen det bar:"
            }
        ],
        [
            {
                "type": "chorus-header",
                "text": "Ref."
            },
            {
                "type": "chorus-lyric",
                "text": "På Blinderen så mistet hun sin uskyld"
            },
            {
                "type": "chorus-lyric",
                "text": "den gang en fuktig sommerkveld."
            },
            {
                "type": "chorus-lyric",
                "text": "Jeg var fortapt, og hun preget av flatfyll,"
            },
            {
                "type": "chorus-lyric",
                "text": "et fåret smil, så gikk det av seg selv."
            },
            {
                "type": "chorus-lyric",
                "text": "Den neste morgen var alt bare smerte"
            },
            {
                "type": "chorus-lyric",
                "text": "og skallebank, da gav hun meg en klem!"
            },
            {
                "type": "chorus-lyric",
                "text": "Ja, det var da jeg gav henne mitt hjerte"
            },
            {
                "type": "chorus-lyric",
                "text": "på Blinderen Studenterhjem."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Alt jeg har nå er minner om sølecup og drank,"
            },
            {
                "type": "lyric",
                "text": "om velferd, vestalinner, revy og hjertebank."
            },
            {
                "type": "lyric",
                "text": "Og nå vil jeg tilbake,"
            },
            {
                "type": "lyric",
                "text": "forkvaklet er jeg klar."
            },
            {
                "type": "lyric",
                "text": "For jeg vender hjem  - til mitt Blinderen.."
            },
            {
                "type": "lyric",
                "text": "Ja, det er alt jeg har:"
            }
        ],
        [
            {
                "type": "chorus-header",
                "text": "Ref."
            },
            {
                "type": "chorus-lyric",
                "text": "Ja, hjertet mitt er Blinderens, alene."
            },
            {
                "type": "chorus-lyric",
                "text": "Jeg burde aldri flyttet ut!"
            },
            {
                "type": "chorus-lyric",
                "text": "Min storhetstid var Bruket å betjene,"
            },
            {
                "type": "chorus-lyric",
                "text": "Studenterhjem har gjort meg til en stut."
            },
            {
                "type": "chorus-lyric",
                "text": "Ja, det var her jeg drakk meg full og skålte"
            },
            {
                "type": "chorus-lyric",
                "text": "på grisefyll  -  i gallauniform."
            },
            {
                "type": "chorus-lyric",
                "text": "På Blinderen sto sjelen min og strålte"
            },
            {
                "type": "chorus-lyric",
                "text": "for det tok hjertet mitt med storm."
            }
        ],
        [
            {
                "type": "chorus-header",
                "text": "Ref."
            },
            {
                "type": "chorus-lyric",
                "text": "Ja, hjertet mitt er Blinderens alene"
            },
            {
                "type": "chorus-lyric",
                "text": "Med all sin prakt og tradisjon."
            },
            {
                "type": "chorus-lyric",
                "text": "Jeg er fortapt i hele fenomenet:"
            },
            {
                "type": "chorus-lyric",
                "text": "Studenterhjem med stil og suggesjon."
            },
            {
                "type": "chorus-lyric",
                "text": "Her var det ball og nydelige kvinner"
            },
            {
                "type": "chorus-lyric",
                "text": "Som alltid kom, og aldri dro avsted."
            },
            {
                "type": "chorus-lyric",
                "text": "På Blinderen, så ble jeg til en vinner,"
            },
            {
                "type": "chorus-lyric",
                "text": "for der fant hjertet mitt sin fred."
            }
        ]
    ]
}
