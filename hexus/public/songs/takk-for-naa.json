        [
            "Takk for nå.",
            "Det var alt kjære venner,",
            "og hva så?",
            "Kanskje syn’s du du kjenner oss nå,",
            "og kanskje har vi",
            "kunnet bidra med vår fantasi."
        ],
        [
            "Takk for nå.",
            "Lyset slukkes på scenen,",
            "og hva så?",
            "Du har sluttet å le, men",
            "husk på:",
            "The show must go on!",
            "Vår revy er livet på lån."
        ],
        [
            "Takk for nå.",
            "Det var moro forresten,",
            "og hva så?",
            "La oss fortsette festen",
            "for nå",
            "er Blinderen ditt.",
            "Så ta for deg med god appetitt."
        ]
