    "contents": [
        [
            {
                "type": "lyric",
                "text": "Takk for nå."
            },
            {
                "type": "lyric",
                "text": "Det var alt kjære venner,"
            },
            {
                "type": "lyric",
                "text": "og hva så?"
            },
            {
                "type": "lyric",
                "text": "Kanskje syn’s du du kjenner oss nå,"
            },
            {
                "type": "lyric",
                "text": "og kanskje har vi"
            },
            {
                "type": "lyric",
                "text": "kunnet bidra med vår fantasi."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Takk for nå."
            },
            {
                "type": "lyric",
                "text": "Lyset slukkes på scenen,"
            },
            {
                "type": "lyric",
                "text": "og hva så?"
            },
            {
                "type": "lyric",
                "text": "Du har sluttet å le, men"
            },
            {
                "type": "lyric",
                "text": "husk på:"
            },
            {
                "type": "lyric",
                "text": "The show must go on!"
            },
            {
                "type": "lyric",
                "text": "Vår revy er livet på lån."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Takk for nå."
            },
            {
                "type": "lyric",
                "text": "Det var moro forresten,"
            },
            {
                "type": "lyric",
                "text": "og hva så?"
            },
            {
                "type": "lyric",
                "text": "La oss fortsette festen"
            },
            {
                "type": "lyric",
                "text": "for nå"
            },
            {
                "type": "lyric",
                "text": "er Blinderen ditt."
            },
            {
                "type": "lyric",
                "text": "Så ta for deg med god appetitt."
            }
        ]
    ]
}
