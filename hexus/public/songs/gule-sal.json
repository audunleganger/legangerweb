    "contents": [
        [
            {
                "type": "lyric",
                "text": "Borte på Vinder’n, no’n meter fra her"
            },
            {
                "type": "lyric",
                "text": "ligger et sykehus blant eldgamle trær."
            },
            {
                "type": "lyric",
                "text": "Vakker er parken og vakkert vårt bygg,"
            },
            {
                "type": "lyric",
                "text": "jeg sier: senga ved veggen på gule sal er stygg!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "På gule sal,"
            },
            {
                "type": "lyric",
                "text": "ingen som hører deg."
            },
            {
                "type": "lyric",
                "text": "På gule sal,"
            },
            {
                "type": "lyric",
                "text": "alt bare sløver deg."
            },
            {
                "type": "lyric",
                "text": "På gule sal,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Sprø er vi alle, det vet vi så godt."
            },
            {
                "type": "lyric",
                "text": "Det at vi vet det sier legen er flott."
            },
            {
                "type": "lyric",
                "text": "Litt gal må man være, sier vår psykolog,"
            },
            {
                "type": "lyric",
                "text": "første mai går han rundt i et m-l-er tog."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "M-L-er tog,"
            },
            {
                "type": "lyric",
                "text": "eksamen fra Blinderen."
            },
            {
                "type": "lyric",
                "text": "M-L-er tog,"
            },
            {
                "type": "lyric",
                "text": "villa på Vinderen."
            },
            {
                "type": "lyric",
                "text": "M-L-er tog,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Onsdag og søndag kommer folk på visitt."
            },
            {
                "type": "lyric",
                "text": "Vi deler presanger, hver smule og smitt."
            },
            {
                "type": "lyric",
                "text": "Noen får blomster, men hvis ikke så gjør"
            },
            {
                "type": "lyric",
                "text": "det oss ingenting, vi har nevroser fra før."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Nevroser fra før,"
            },
            {
                "type": "lyric",
                "text": "vakrest blant rosene."
            },
            {
                "type": "lyric",
                "text": "Nevroser fra før,"
            },
            {
                "type": "lyric",
                "text": "mellom psykosene."
            },
            {
                "type": "lyric",
                "text": "nevroser fra før,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Og skulle du føle deg litt nedfor en dag,"
            },
            {
                "type": "lyric",
                "text": "så sier din lege det er kun et bedrag."
            },
            {
                "type": "lyric",
                "text": "Men bråker du noe, så kaller’n deg gal,"
            },
            {
                "type": "lyric",
                "text": "og da dempes humøret med seler og Truxal."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Seler og Truxal,"
            },
            {
                "type": "lyric",
                "text": "mmm, det lukter effektivt."
            },
            {
                "type": "lyric",
                "text": "Seler og Truxal,"
            },
            {
                "type": "lyric",
                "text": "valget er alternativt."
            },
            {
                "type": "lyric",
                "text": "Seler og Truxal..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Mens jeg var hjemme så sov jeg så bra"
            },
            {
                "type": "lyric",
                "text": "sammen med kona og en Grand Danois."
            },
            {
                "type": "lyric",
                "text": "Med tolv mann på rommet ble søvnen litt sen,"
            },
            {
                "type": "lyric",
                "text": "men det fiksa dem lett med litt Meprodiphen."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Meprodiphen,"
            },
            {
                "type": "lyric",
                "text": "søvnens gudinne er."
            },
            {
                "type": "lyric",
                "text": "Meprodiphen,"
            },
            {
                "type": "lyric",
                "text": "finner du nok av her."
            },
            {
                "type": "lyric",
                "text": "Meprodiphen..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Litt praktisk arbeid er god terapi,"
            },
            {
                "type": "lyric",
                "text": "vi høvler og snekrer med stor energi."
            },
            {
                "type": "lyric",
                "text": "De tok mitt barberblad og også mitt slips,"
            },
            {
                "type": "lyric",
                "text": "men vil du skjære deg her, gir sløyden masse tips."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Hel masse tips,"
            },
            {
                "type": "lyric",
                "text": "høvel og kniv og sag."
            },
            {
                "type": "lyric",
                "text": "Hel masse tips,"
            },
            {
                "type": "lyric",
                "text": "høvel og kniv og sag."
            },
            {
                "type": "lyric",
                "text": "Hel masse tips..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Mange av oss her er lagt inn med tvang,"
            },
            {
                "type": "lyric",
                "text": "men noen er kommet hit av egen trang."
            },
            {
                "type": "lyric",
                "text": "Vil du bli lagt inn, så tar du en risk:"
            },
            {
                "type": "lyric",
                "text": "Her må det jernhelse til"
            },
            {
                "type": "lyric",
                "text": "– om du skal bli frisk."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Du skal bli frisk,"
            },
            {
                "type": "lyric",
                "text": "du tar en jævla risk."
            },
            {
                "type": "lyric",
                "text": "Du skal bli frisk,"
            },
            {
                "type": "lyric",
                "text": "du tar en jævla risk."
            },
            {
                "type": "lyric",
                "text": "Du skal bli frisk..."
            }
        ]
