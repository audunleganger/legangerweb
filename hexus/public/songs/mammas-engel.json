    "contents": [
        [
            {
                "type": "lyric",
                "text": "Mamma er så stolt av “lille gullet” "
            },
            {
                "type": "lyric",
                "text": "(...det er meg...)"
            },
            {
                "type": "lyric",
                "text": "Slik en blyg og dydig pike, "
            },
            {
                "type": "lyric",
                "text": "som har alles sympati."
            },
            {
                "type": "lyric",
                "text": "(...ja, det er meg...)"
            },
            {
                "type": "lyric",
                "text": "Mamma tror at jeg er som en engel,"
            },
            {
                "type": "lyric",
                "text": "og fra jeg flyttet inn på Blindern, "
            },
            {
                "type": "lyric",
                "text": "har studert teologi."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Så hvis du skulle støte på min mamma,"
            },
            {
                "type": "lyric",
                "text": "Ikke si at “hennes engel”"
            },
            {
                "type": "lyric",
                "text": "gjør litt andre ting på si’."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Hysj,hysj, spar min mamma,"
            },
            {
                "type": "lyric",
                "text": "“Fysj, fysj” svarer mamma"
            },
            {
                "type": "lyric",
                "text": "hvis hun skulle få greie på no’..."
            },
            {
                "type": "lyric",
                "text": "Hvis du holdt det hem’lig"
            },
            {
                "type": "lyric",
                "text": "ble det mest bekvem’lig"
            },
            {
                "type": "lyric",
                "text": "for oss begge, vil jeg tro."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg bryter alt jeg lovet kjære mamma,"
            },
            {
                "type": "lyric",
                "text": "hun ville ikke greid å leve med den skamma."
            },
            {
                "type": "lyric",
                "text": "Så vær snill, min kjære, lov på tro og ære,"
            },
            {
                "type": "lyric",
                "text": "så kan vi to kose oss i fred."
            },
            {
                "type": "lyric",
                "text": "Du kan si til pappa hva du vil"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "For han har bodd her og var gild"
            },
            {
                "type": "lyric",
                "text": "Da han sto slik i seksti-tre."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Mamma tror at mine interesser"
            },
            {
                "type": "lyric",
                "text": "kun er salmesang og messer"
            },
            {
                "type": "lyric",
                "text": "og sjelesorg og bønn."
            },
            {
                "type": "lyric",
                "text": "Mamma tror jeg går på smale stier"
            },
            {
                "type": "lyric",
                "text": "Og blir “stukket” kun av bier..."
            },
            {
                "type": "lyric",
                "text": "og at ellers er jeg “grønn”."
            },
            {
                "type": "lyric",
                "text": "Så hvis du skulle støte på min mamma,"
            },
            {
                "type": "lyric",
                "text": "Bare glem meg med det samma,"
            },
            {
                "type": "lyric",
                "text": "og da skal du få din lønn."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Hysj, hysj, spar min mamma"
            },
            {
                "type": "lyric",
                "text": "“Fysj, fysj” svarer mamma"
            },
            {
                "type": "lyric",
                "text": "hvis hun skulle få greie på no’."
            },
            {
                "type": "lyric",
                "text": "Hvis du holdt det hem’lig"
            },
            {
                "type": "lyric",
                "text": "ble det mest bekvem’lig"
            },
            {
                "type": "lyric",
                "text": "for oss begge, vil jeg tro."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "For hvis du sier no’ er jeg ille ute."
            },
            {
                "type": "lyric",
                "text": "Og eventyret ditt har da nådd"
            },
            {
                "type": "lyric",
                "text": "snipp, snapp, snute."
            },
            {
                "type": "lyric",
                "text": "Så vær snill, da søten,"
            },
            {
                "type": "lyric",
                "text": "du kan gå rundt grøten,"
            },
            {
                "type": "lyric",
                "text": "så kan musen danse på ditt bord."
            },
            {
                "type": "lyric",
                "text": "Du kan si til mormor hva du vil,"
            },
            {
                "type": "lyric",
                "text": "for hun er blitt imbecil,"
            },
            {
                "type": "lyric",
                "text": "men ikke si no’ til min mor."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Du kan si til onkel alt du vet,"
            },
            {
                "type": "lyric",
                "text": "Du slipper være for konkret,"
            },
            {
                "type": "lyric",
                "text": "for hver kveld sitter’n der og glor."
            },
            {
                "type": "lyric",
                "text": "Og mine brødre vil nok flire rått,"
            },
            {
                "type": "lyric",
                "text": "for de står der med hver sin spot!"
            },
            {
                "type": "lyric",
                "text": "Men vær så snill, da smukken,"
            },
            {
                "type": "lyric",
                "text": "for jeg går nok dukken "
            },
            {
                "type": "lyric",
                "text": "hvis du sladrer til min mor…"
            }
        ]
    ]
