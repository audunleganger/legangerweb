    },
    "contents": [
        [
            {
                "type": "italic underline no-line-break",
                "text": "Han:"
            },
            {
                "type": "lyric",
                "text": "\tDu og jeg har en himmel sammen."
            },
            {
                "type": "italic underline no-line-break",
                "text": "Hun:"
            },
            {
                "type": "lyric",
                "text": "\tDu og jeg holder alltid liv i flammen"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Begge:"
            },
            {
                "type": "lyric",
                "text": "\tAmor og Venus vinker langs vår vei"
            }
        ],
        [
            {
                "type": "italic underline no-line-break",
                "text": "Han:"
            },
            {
                "type": "lyric",
                "text": "\tDine smil er bensin på bålet"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Hun:"
            },
            {
                "type": "lyric",
                "text": "\tDet er du som har fått meg til å stråle!"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Begge:"
            },
            {
                "type": "lyric",
                "text": "\tSolen står høyt, så lenge jeg har deg!"
            },
            {
                "type": "lyric",
                "text": "\t\tNå er vi her, nå er vi sammen."
            },
            {
                "type": "lyric",
                "text": "\t\tAlle kan se oss, og alle glor!"
            },
            {
                "type": "lyric",
                "text": "\t\tOg alle kan høre når jeg hvisker ømt:"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Han:"
            },
            {
                "type": "lyric",
                "text": "\tStore ord"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Hun:"
            },
            {
                "type": "lyric",
                "text": "\tStore ord, store ord."
            }
        ],
        [
            {
                "type": "instruction",
                "text": "(avstanden øker mellom dem)"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Han:"
            },
            {
                "type": "lyric",
                "text": "\tDu og jeg deler evig lykke."
            },
            {
                "type": "italic underline no-line-break",
                "text": "Hun:"
            },
            {
                "type": "lyric",
                "text": "\tKjærlighet lar seg aldri undertrykke!"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Begge:"
            },
            {
                "type": "lyric",
                "text": "\tBare et blikk fra deg gir alle svar."
            },
            {
                "type": "italic underline no-line-break",
                "text": "Han:"
            },
            {
                "type": "lyric",
                "text": "\tDu og jeg (Hun: du og jeg...)"
            },
            {
                "type": "lyric",
                "text": "\t\tDu kan best forstå meg…"
            },
            {
                "type": "lyric",
                "text": "\t\tDu og jeg (Hun: du og jeg...)"
            },
            {
                "type": "lyric",
                "text": "\t\tIkke syng i kjeften på meg!"
            },
            {
                "type": "lyric",
                "text": "\t\tJeg er din helt..."
            },
            {
                "type": "italic underline no-line-break",
                "text": "Hun:"
            },
            {
                "type": "lyric",
                "text": "\t... og jeg din superstar!"
            }
        ],
        [
            {
                "type": "italic underline no-line-break",
                "text": "Begge:"
            },
            {
                "type": "lyric",
                "text": "\tNatt blir til dag, drømmen får vinger"
            },
            {
                "type": "lyric",
                "text": "\t\tHele min sjel åpenbarer seg."
            },
            {
                "type": "lyric",
                "text": "\t\tDet alle kan høre når jeg hvisker ømt."
            },
            {
                "type": "lyric",
                "text": "\t\tDet trengs ikke glitter og gods eller gull."
            },
            {
                "type": "italic underline no-line-break",
                "text": "Han:"
            },
            {
                "type": "lyric",
                "text": "\tBare deg"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Hun:"
            },
            {
                "type": "lyric",
                "text": "\tBare meg"
            },
            {
                "type": "italic underline no-line-break",
                "text": "Begge:"
            },
            {
                "type": "lyric",
                "text": "\tBare meg. Bare vi to har - det vi to har."
            }
        ]
    ]
}
