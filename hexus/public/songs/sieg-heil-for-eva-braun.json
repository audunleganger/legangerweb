    "contents": [
        [
            {
                "type": "lyric",
                "text": "I en liten kneipe, i Berlin et sted"
            },
            {
                "type": "lyric",
                "text": "sitter jeg ved baren og nyter kveldens fred,"
            },
            {
                "type": "lyric",
                "text": "minnes mein Führer og mein Mann,"
            },
            {
                "type": "lyric",
                "text": "das dritte Reich, mein Vaterland,"
            },
            {
                "type": "lyric",
                "text": "Mitt navn er Eva Braun,"
            },
            {
                "type": "lyric",
                "text": "mitt navn er Eva Braun."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "I min lille kneipe er Adolf favoritt:"
            },
            {
                "type": "lyric",
                "text": "ned med parasitter og ut med fremmed dritt."
            },
            {
                "type": "lyric",
                "text": "Gi oss igjen den sterke mann."
            },
            {
                "type": "lyric",
                "text": "Et jernhårdt grep, om folk og land."
            },
            {
                "type": "lyric",
                "text": "Sieg heil for Eva Braun,"
            },
            {
                "type": "lyric",
                "text": "Sieg heil for Eva Braun."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Slutt med pengesølet til trygd og velferdsskrot,"
            },
            {
                "type": "lyric",
                "text": "spreng vekk byens rådhus og demokratisk rot."
            },
            {
                "type": "lyric",
                "text": "Brenn falske bøker, gjør det hett."
            },
            {
                "type": "lyric",
                "text": "Mitt motto er “De sterkes rett”."
            },
            {
                "type": "lyric",
                "text": "Sieg heil for Eva Braun,"
            },
            {
                "type": "lyric",
                "text": "Sieg heil for Eva Braun."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg har mange kneiper i så mange land."
            },
            {
                "type": "lyric",
                "text": "Jeg kjenner ingen grenser for hatets egen klan."
            },
            {
                "type": "lyric",
                "text": "Ser du meg ikke ved ditt bord?"
            },
            {
                "type": "lyric",
                "text": "Jeg er hos deg når hatet gror,"
            },
            {
                "type": "lyric",
                "text": "din egen Eva Braun,"
            },
            {
                "type": "lyric",
                "text": "din egen Eva Braun."
            }
        ]
    ]
