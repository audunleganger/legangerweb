    "contents": [
        [
            {
                "type": "lyric",
                "text": "Så De bukkehornene? Fant De gull blant kornene?"
            },
            {
                "type": "lyric",
                "text": "Følte De det hele tiden?"
            },
            {
                "type": "lyric",
                "text": "En som midt i sangene, stønnende og stangende"
            },
            {
                "type": "lyric",
                "text": "satte Dem et horn i siden?"
            },
            {
                "type": "lyric",
                "text": "Rens da Deres stemme"
            },
            {
                "type": "lyric",
                "text": "og få med Dem et evigvarig minne  -"
            },
            {
                "type": "lyric",
                "text": "og kom bli med…"
            },
            {
                "type": "lyric",
                "text": "i en jublende hyllest til"
            },
            {
                "type": "lyric",
                "text": "vår majestetiske blinde… "
            }
        ],
        [
            {
                "type": "lyric",
                "text": "BUKK   -"
            },
            {
                "type": "lyric",
                "text": "bring vår revy stor ære og glede:"
            },
            {
                "type": "lyric",
                "text": "kron kveldens meny: vær bare til stede."
            },
            {
                "type": "lyric",
                "text": "«Høybukken er her igjen»"
            },
            {
                "type": "lyric",
                "text": "«Bli her til i morgen»!"
            },
            {
                "type": "lyric",
                "text": "Stang alle på Blinderen  -"
            },
            {
                "type": "lyric",
                "text": "inn i våren! "
            }
        ],
        [
            {
                "type": "lyric",
                "text": "BUKK   -"
            },
            {
                "type": "lyric",
                "text": "bring vår revy stor ære og glede:"
            },
            {
                "type": "lyric",
                "text": "kron kveldens meny: vær bare til stede!"
            },
            {
                "type": "lyric",
                "text": "Høybukken er her igjen,"
            },
            {
                "type": "lyric",
                "text": "Hils De og,  -  min venn  -"
            },
            {
                "type": "lyric",
                "text": "vår BUKK  -"
            },
            {
                "type": "lyric",
                "text": "Bukkende BUKK   -"
            },
            {
                "type": "lyric",
                "text": "Bukkende B U K K  -"
            },
            {
                "type": "lyric",
                "text": "Bukkende B  U  K  K !"
            }
        ]
    ]
