    "contents": [
        [
            {
                "type": "lyric",
                "text": "Jeg har flyttet inn på 503 og her er det veldig koseli’"
            },
            {
                "type": "lyric",
                "text": "Det er himmelen for en som meg"
            },
            {
                "type": "lyric",
                "text": "som vil ha det stille rundt seg"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Ingen dyr og ingen fyr, for jeg er veldig prippen!"
            },
            {
                "type": "lyric",
                "text": "Og jeg har vinduskrok med skråtak til"
            },
            {
                "type": "lyric",
                "text": "En kosekrok (wiiii!) og med en bok"
            },
            {
                "type": "lyric",
                "text": "er jeg i drømmelandet helt inntil "
            },
            {
                "type": "lyric",
                "text": "(Synger opera)"
            },
            {
                "type": "lyric",
                "text": "Hva var det for no’?"
            },
            {
                "type": "lyric",
                "text": "Naboen under tro?"
            },
            {
                "type": "lyric",
                "text": "Argh! Fra en opera - «wannabe” på 403"
            },
            {
                "type": "lyric",
                "text": "En matiné og en kantate, Wagners-ringende traviate"
            },
            {
                "type": "lyric",
                "text": "(Opera med tryllefløyten- referanser)"
            },
            {
                "type": "lyric",
                "text": "Min første natt på 503,"
            },
            {
                "type": "lyric",
                "text": "og jeg holdt ut med Puccini"
            },
            {
                "type": "lyric",
                "text": "så jeg laget meg en kopp med te"
            },
            {
                "type": "lyric",
                "text": "croshét til den er «fini»,  hihi!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Kvart på ti og ingen lyd!"
            },
            {
                "type": "lyric",
                "text": "Bortsett fra klokketikking"
            },
            {
                "type": "lyric",
                "text": "Så nå har jeg tenkt å legge meg"
            },
            {
                "type": "lyric",
                "text": "En deilig seng (ÅH!) med plass i fleng"
            },
            {
                "type": "lyric",
                "text": "En strekk, en gjesp og jeg er vekk inntil "
            },
            {
                "type": "lyric",
                "text": "(Synger og skatter jazz som ender i et høyt "
            }
        ],
        [
            {
                "type": "lyric",
                "text": "WOHOOO!)"
            },
            {
                "type": "lyric",
                "text": "Jenten ovenpå"
            },
            {
                "type": "lyric",
                "text": "Gud! Jeg går i stå!"
            },
            {
                "type": "lyric",
                "text": "Blåser ned fra 603 en matiné"
            },
            {
                "type": "lyric",
                "text": "Hun skatter som om hun er Ella, "
            },
            {
                "type": "lyric",
                "text": "og hun der svarer à capella!"
            },
            {
                "type": "lyric",
                "text": "(Synger jazz og opera om hverandre samtidig)"
            },
            {
                "type": "lyric",
                "text": "Hever sjelden stemmen, jeg"
            },
            {
                "type": "lyric",
                "text": "jeg er som regel snill og grei, men…"
            },
            {
                "type": "lyric",
                "text": "Kan dere kanskje, BARE kanskje…."
            },
            {
                "type": "lyric",
                "text": "vær så snill å…… STOOOOOOOOOOOPP!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Gud, det var deilig. "
            },
            {
                "type": "lyric",
                "text": "(Musikken modulerer)"
            },
            {
                "type": "lyric",
                "text": "STOOOOOOOOOOOOOOOPP!!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "4, 5 og 603 er en elendig triooooo, aaaargh!"
            },
            {
                "type": "lyric",
                "text": "Ingen perfekt harmoni!"
            },
            {
                "type": "lyric",
                "text": "For natten lang vi synger: "
            },
            {
                "type": "lyric",
                "text": "(Jazz og opera om hverandre) "
            },
            {
                "type": "lyric",
                "text": "Men nå skal nok det bli stille"
            },
            {
                "type": "lyric",
                "text": "For nå har jeg gitt dem en pille"
            },
            {
                "type": "lyric",
                "text": "Med vennlig hilsen 503!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "HYSJ!!!!"
            }
        ]
    ]
