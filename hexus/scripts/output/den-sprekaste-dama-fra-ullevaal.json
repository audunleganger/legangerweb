    "contents": [
        [
            {
                "type": "lyric",
                "text": "Ja, eg var visst på veg bort til Vinderen,"
            },
            {
                "type": "lyric",
                "text": "då dei tok meg til «peiskveld» på Blinderen."
            },
            {
                "type": "lyric",
                "text": "Dei var fire mann, hadde øl i eit spann,"
            },
            {
                "type": "lyric",
                "text": "og de i hadde brendevin, ekte Gordon’s gin..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Og gutane var visst i kvinnfolkbeit,"
            },
            {
                "type": "lyric",
                "text": "og litt sprit, det er jaggu det beste eg veit."
            },
            {
                "type": "lyric",
                "text": "Så du må veta det, eg vart med,"
            },
            {
                "type": "lyric",
                "text": "og sidan har eg altså vori her."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "(Og korkje eg eller gutane ha angra på det, "
            },
            {
                "type": "lyric",
                "text": "for – )"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Eg er sprekaste dama frå Ullevål"
            },
            {
                "type": "lyric",
                "text": "når eg glefser i mannfolk og brukar vold."
            },
            {
                "type": "lyric",
                "text": "Eg triver og river i øret og håret, magan og låret,"
            },
            {
                "type": "lyric",
                "text": "hovud og kropp, ja eg eter dei opp..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Eg har barskaste beina i heile by’n,"
            },
            {
                "type": "lyric",
                "text": "ja, det høgre er kjend i frå Voss til Stryn."
            },
            {
                "type": "lyric",
                "text": "Skal eg ta han ut, vil du sjå han gut, ?"
            },
            {
                "type": "lyric",
                "text": "(...det var denna som fekk dyrlægen heime til "
            },
            {
                "type": "lyric",
                "text": "å døy av slag...),"
            },
            {
                "type": "lyric",
                "text": "for han var ein stakkars fyr, berre vant til kyr."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Og får eg ikkje nok av min sprit-rasjon,"
            },
            {
                "type": "lyric",
                "text": "ja, da kjøper eg skokrem på moderasjon"
            },
            {
                "type": "lyric",
                "text": "er studentikos, et ansjos."
            },
            {
                "type": "lyric",
                "text": "Å, kor eg trives her på Blinderen!"
            }
        ]
    ]
}