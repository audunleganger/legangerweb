    "contents": [
        [
            {
                "type": "lyric",
                "text": "Vi har vært på alle tiders grøvste utdrikningskalas  -"
            },
            {
                "type": "lyric",
                "text": "vi var innom Møllergata og en doktor-disputas,"
            },
            {
                "type": "lyric",
                "text": "vi fikk følge hjem til Blindern av en purk som ville se"
            },
            {
                "type": "lyric",
                "text": "om dette hjem vi kom ifra var like jævlig som oss tre…"
            },
            {
                "type": "lyric",
                "text": "Men hva purken ikke visste var at festen som vi holdt"
            },
            {
                "type": "lyric",
                "text": "var til ære for en Blinderngutt og alt han har forvoldt."
            },
            {
                "type": "lyric",
                "text": "Så da gjengen gikk i gåsegang mot Blindernbakken opp"
            },
            {
                "type": "lyric",
                "text": "kom hedersgjesten ut i vinduet på Hovedbyggets topp!"
            },
            {
                "type": "lyric",
                "text": "«Her er alkohol til tusen!»"
            },
            {
                "type": "lyric",
                "text": "skrek han lallende i rusen"
            },
            {
                "type": "lyric",
                "text": "og så åpna slasken slusen,"
            },
            {
                "type": "lyric",
                "text": "og så datt han ut."
            },
            {
                "type": "lyric",
                "text": "Og vi gravd’n opp av bedet,"
            },
            {
                "type": "lyric",
                "text": "det var meget varmt der nede."
            },
            {
                "type": "lyric",
                "text": "Purken sa i angst og vrede:"
            },
            {
                "type": "lyric",
                "text": "«Detta er forbudt!»"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "«Er’e  slik dem har’e her?» sa purken da vi bar ham opp,"
            },
            {
                "type": "lyric",
                "text": "i det samme kom det fire mann med råsprit i en kopp."
            },
            {
                "type": "lyric",
                "text": "«Smak på husets små produkter» hilste de vår nye gjest,"
            },
            {
                "type": "lyric",
                "text": "«men se å få av uniformen, mann  -  da liker vi Dem best!»"
            },
            {
                "type": "lyric",
                "text": "En person som sto i postrommet ble rødstripa av skrekk"
            },
            {
                "type": "lyric",
                "text": "da han så vår brudgom tatovert med mønje, tusj og blekk."
            },
            {
                "type": "lyric",
                "text": "Han var mellomgrå og gul og blå og mange «stygge ting»,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "det som mangla oppi hue var blant annet noen sting."
            },
            {
                "type": "lyric",
                "text": "«Det er all right med no’n gjester"
            },
            {
                "type": "lyric",
                "text": "og med noen fyllefester"
            },
            {
                "type": "lyric",
                "text": "i et arbeidssomt semester"
            },
            {
                "type": "lyric",
                "text": "men nå er’e slutt,»"
            },
            {
                "type": "lyric",
                "text": "sa vår politikonstabel,"
            },
            {
                "type": "lyric",
                "text": "han var blitt litt irritabel"
            },
            {
                "type": "lyric",
                "text": "og han blåste i sin snabel:"
            },
            {
                "type": "lyric",
                "text": "«Detta er forbudt!»"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "I det samme hørte vi på gangen raske, spente skritt  -"
            },
            {
                "type": "lyric",
                "text": "det var Falchens Redningskorps som kom på"
            },
            {
                "type": "lyric",
                "text": "nattvandrings-visitt."
            },
            {
                "type": "lyric",
                "text": "Hedersgjesten sov i dusjen, for i senga var det fullt,"
            },
            {
                "type": "lyric",
                "text": "og under skrivebordet rydda purken opp i en tumult."
            },
            {
                "type": "lyric",
                "text": "Ingen merka at Kollegiet brøt inn i vår idyll,"
            },
            {
                "type": "lyric",
                "text": "ingen hørte Præces rope at han ikke likte fyll."
            },
            {
                "type": "lyric",
                "text": "Ingen ante at vår frokost allerede var servert,"
            },
            {
                "type": "lyric",
                "text": "ingen brød seg om at brudgommen var rett og slett krepert."
            },
            {
                "type": "lyric",
                "text": "«Jeg må heftig protestere!»"
            },
            {
                "type": "lyric",
                "text": "gaula Falch og noen flere,"
            },
            {
                "type": "lyric",
                "text": "«Skamme dere skulle dere,"
            },
            {
                "type": "lyric",
                "text": "nå er festen slutt!»"
            },
            {
                "type": "lyric",
                "text": "«Nå går vi og henter Jansen!»"
            },
            {
                "type": "lyric",
                "text": "Da kom purken ut av trancen,"
            },
            {
                "type": "lyric",
                "text": "kommanderte: «Stans’n, stans’n,"
            },
            {
                "type": "lyric",
                "text": "detta er forbudt!»"
            }
        ]
    ]
}