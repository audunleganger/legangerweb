    "contents": [
        [
            {
                "type": "lyric",
                "text": "Her står jeg i min ungdoms prakt –"
            },
            {
                "type": "lyric",
                "text": "kledd i Blinderns enkle drakt –"
            },
            {
                "type": "lyric",
                "text": "et praktisk, godt studentantrekk –"
            },
            {
                "type": "lyric",
                "text": "sydd hos Widén."
            },
            {
                "type": "lyric",
                "text": "Og jeg studerer medisin,"
            },
            {
                "type": "lyric",
                "text": "samt Cointreau og edel vin,"
            },
            {
                "type": "lyric",
                "text": "små cocktails med en dash av gin –"
            },
            {
                "type": "lyric",
                "text": "her har De meg."
            },
            {
                "type": "lyric",
                "text": "Jeg bor spartansk – javel –"
            },
            {
                "type": "lyric",
                "text": "men jeg herdes på kropp og på sjel –"
            },
            {
                "type": "lyric",
                "text": "og faller tiden lang"
            },
            {
                "type": "lyric",
                "text": "sklir jeg i Buicken min ned til Grand."
            },
            {
                "type": "lyric",
                "text": "Der har jeg så fra klokka 3"
            },
            {
                "type": "lyric",
                "text": "noen drinks på chambre séparée,"
            },
            {
                "type": "lyric",
                "text": "så mye kvinns som det skal te’"
            },
            {
                "type": "lyric",
                "text": "for mannens fred."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "l Universitas – på prent,"
            },
            {
                "type": "lyric",
                "text": "ja – der sto det noe sånt – omtrent –"
            },
            {
                "type": "lyric",
                "text": "at Blindern var et tilholdssted"
            },
            {
                "type": "lyric",
                "text": "for bare snobb."
            },
            {
                "type": "lyric",
                "text": "Professor Jansen ble så sinna, så –"
            },
            {
                "type": "lyric",
                "text": "han svarte bladet comme il faut"
            },
            {
                "type": "lyric",
                "text": "at «Sånt no går da ikke an –"
            },
            {
                "type": "lyric",
                "text": "Nei – vet De hva!»"
            },
            {
                "type": "lyric",
                "text": "Vår husfar ga saken syn –"
            },
            {
                "type": "lyric",
                "text": "«Gutta her har’ke opphav med gryn,"
            },
            {
                "type": "lyric",
                "text": "for i gjennomsnitt det er"
            },
            {
                "type": "lyric",
                "text": "snaut en halv million bakom hver!»"
            },
            {
                "type": "lyric",
                "text": "Og – kjære Universitas;"
            },
            {
                "type": "lyric",
                "text": "kom hit når lørdagsmidda’n tas"
            },
            {
                "type": "lyric",
                "text": "med pottit, sild og vann i glass –"
            },
            {
                "type": "lyric",
                "text": "og plenty plass!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Nei – Blindern er’ke stedet nå"
            },
            {
                "type": "lyric",
                "text": "som våre fedre bodde på –"
            },
            {
                "type": "lyric",
                "text": "snart er det blitt en almen skikk"
            },
            {
                "type": "lyric",
                "text": "å bruke trikk."
            },
            {
                "type": "lyric",
                "text": "På veggen har vi Munch – og no’ –"
            },
            {
                "type": "lyric",
                "text": "men vi har’ke Michelangelo –"
            },
            {
                "type": "lyric",
                "text": "det viser jo at vi kan bo"
            },
            {
                "type": "lyric",
                "text": "som andre folk."
            },
            {
                "type": "lyric",
                "text": "Og sport er jeg gått inn for nå –"
            },
            {
                "type": "lyric",
                "text": "ja, tenk nå klarer jeg nesten å gå"
            },
            {
                "type": "lyric",
                "text": "bort til Vindern, men da må jeg ha"
            },
            {
                "type": "lyric",
                "text": "en bil – og fire, fem mann til å ta"
            },
            {
                "type": "lyric",
                "text": "imot meg når jeg kommer hjem"
            },
            {
                "type": "lyric",
                "text": "klar til å bære pjolter’n frem –"
            },
            {
                "type": "lyric",
                "text": "og stakkars lille husmor da –"
            },
            {
                "type": "lyric",
                "text": "hvis den er varm. "
            },
            {
                "type": "lyric",
                "text": " "
            }
        ]
    ]
