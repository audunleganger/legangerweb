    "contents": [
        [
            {
                "type": "lyric",
                "text": "-Da jeg kom til verden, gav far firmaet fri resten av dagen."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Min fødsel ble feiret med sigarer og lyserød champagne."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Jeg skulle egentlig ikke vært født,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "men for tredve år siden var det vanskeligere å få abort..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Jeg fikk nok dåpsgaver til å fylle en gullsmedforretning."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Alt jeg fikk, var en spiseskje   –i rustfritt stål..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Som barn likte jeg å leke med de elektriske togene mine."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg hadde et permanent anlegg med til sammen"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "en kilometer med skinner."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Jeg var fascinert av tog. Fra kjøkkenvinduet vårt"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "var det bare et steinkast til Østbanen…"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Da jeg ble konfirmert, hadde jeg vært i atten land."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Jeg dro på klassetur   -  til Åndalsnes …"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Hos oss spilte vi golf og bridge."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Vi spilte fotball på løkka og kastet ett-øres på stikka..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg hadde privatundervisning i fransk."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Og jeg fikk premie for gode resultater..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Far gav meg lommepenger – over sjekk-konto."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg gikk med Aftenposten."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Etter artium begynte jeg ved universitetetet..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg fikk også en slags eksamen – litt forsinket."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Så studerte jeg til forberedende."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg spiste på Frederikke. Annenhver lørdag gikk jeg på kino..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg inntok biffen på Theatercafeen,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "men skulle jeg ha en helaften, var Speilen stedet.  –"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "I 1963 fant jeg ut hvem min far var…"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "- Samme år døde min far. Fem år senere ble familiefirmaet"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "slått konkurs, og forretningsføreren fengslet for underslag"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "på en halv million."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-25 år gammel kjøpte jeg min første dress..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Da jeg ble blakk, sluttet jeg på universitetet."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg innså at jeg aldri kom til å bli noen cand."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg fikk flere tilsagn om stillinger før jeg hadde tatt"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "den endelige eksamen..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-En som hadde kjent far, gav meg en jobb."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "–Jeg avanserte raskt i firmaet..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Jeg fattet aldri interesse for arbeidet mitt."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-I fjor tok sjefen meg opp som kompanjong..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-I fjor ansatte min sjef rasjonaliseringsekspert , -"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "og jeg mistet jobben.    "
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Etter forfremmelsen giftet jeg meg"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "med verdens deiligste sekretær..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Min hybelvertinne har låst inngangsdøren."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg skylder leie for fire måneder."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Nå har min kone og jeg flyttet inn i egen villa..."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-I dag skjedde det jævlige: jeg har fått rekommandert"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "forelegg, og sannsynligvis en sønn."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Nå ringte de akkurat fra Stiftelsen."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Min kone har nedkommet med en velskapt, liten pjokk…"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Flytte sammen med jenta kan jeg ikke."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Da stanser jo de offentlige bidragene."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "-Jeg sender budet ut et ærend:"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "i dag river jeg i sigarer og champagne på alle mann!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "- Ja, ja, ungen skal få lære livets realiteter."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "– MIN SØNN SKAL HA ALT JEG IKKE FIKK SOM BARN..."
            }
        ]
    ]
}