    "contents": [
        [
            {
                "type": "lyric",
                "text": "Jeg har vokst opp i gamle kvartaler"
            },
            {
                "type": "lyric",
                "text": "I en bydel som er blitt sanert."
            },
            {
                "type": "lyric",
                "text": "Der jeg hadde mitt hjem, er det nå plass til dem"
            },
            {
                "type": "lyric",
                "text": "som skal føre utviklinga frem."
            },
            {
                "type": "lyric",
                "text": "De har revet ned alt som er gammelt, "
            },
            {
                "type": "lyric",
                "text": "bygd det opp igjen i betong og glass."
            },
            {
                "type": "lyric",
                "text": "Det er vanskelig å tro at der huset vårt sto"
            },
            {
                "type": "lyric",
                "text": "er det nå blitt parkeringsplass."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg kan’ke forstå at vår by skal forgå."
            },
            {
                "type": "lyric",
                "text": "De som styrer her må være sprø!"
            },
            {
                "type": "lyric",
                "text": "Men hva teller vel jeg, ingen hører på meg, - "
            },
            {
                "type": "lyric",
                "text": "Jeg er død."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "De har tatt fra meg alt det jeg eide,"
            },
            {
                "type": "lyric",
                "text": "de har herjet og brent på min jord."
            },
            {
                "type": "lyric",
                "text": "Nå går alt på profitt, byen flyter i dritt, "
            },
            {
                "type": "lyric",
                "text": "forurensningen blomstrer og gror."
            },
            {
                "type": "lyric",
                "text": "Kanskje er jeg et romantisk gjenferd,"
            },
            {
                "type": "lyric",
                "text": "kanskje er jeg en ut-gammel tosk."
            },
            {
                "type": "lyric",
                "text": "Jeg har levd i en tid som forlengst er forbi."
            },
            {
                "type": "lyric",
                "text": "Den gang gresset var grønt, "
            },
            {
                "type": "lyric",
                "text": "den gang lufta var klar"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg vil stille et spørsmål og krever et svar:"
            },
            {
                "type": "lyric",
                "text": "Hvorfor ødela de det som var byens idé?"
            },
            {
                "type": "lyric",
                "text": "Byen er død."
            }
