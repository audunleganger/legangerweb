    "contents": [
        [
            {
                "type": "lyric",
                "text": "Godaften, kjære venner, dere husker meg "
            },
            {
                "type": "lyric",
                "text": "igjen fra allmannamøtet i fjor?"
            },
            {
                "type": "lyric",
                "text": "Jeg er hele fakultetets spesielle, gode venn"
            },
            {
                "type": "lyric",
                "text": "som nyter alles tillit, og jeg tror "
            },
            {
                "type": "lyric",
                "text": "dere valgte klokt som valgte meg til tillitsmann:"
            },
            {
                "type": "lyric",
                "text": "Jeg har tatt det svært alvorlig, og jeg tror jeg kan "
            },
            {
                "type": "lyric",
                "text": "forsikre om at deres interesser"
            },
            {
                "type": "lyric",
                "text": "er ivaretatt med kløkt og med finesser. "
            }
        ],
        [
            {
                "type": "lyric",
                "text": "D’er lunt i ‘Tingets konferanse-rom"
            },
            {
                "type": "lyric",
                "text": "der kompetanser som jeg selv"
            },
            {
                "type": "lyric",
                "text": "tar plass. Og de som gav meg mitt mandat"
            },
            {
                "type": "lyric",
                "text": "ser i sin delegat: seg selv!"
            },
            {
                "type": "lyric",
                "text": "Hver gang jeg skal votere"
            },
            {
                "type": "lyric",
                "text": "for dere med flere"
            },
            {
                "type": "lyric",
                "text": "da hører jeg min indre stemme si:"
            },
            {
                "type": "lyric",
                "text": "«Vi har et sundt student-demokrati»  "
            },
            {
                "type": "lyric",
                "text": " "
            },
            {
                "type": "lyric",
                "text": "‘Beklager min kontakt med dere ikke er på topp;"
            },
            {
                "type": "lyric",
                "text": "vervet tar dagen og natten - !"
            },
            {
                "type": "lyric",
                "text": "om jeg skal kunne grei’å greie ut og greie opp"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "og delta i utbygningsdebatten."
            },
            {
                "type": "lyric",
                "text": "Og utrede og og innlede og fremme forslag om"
            },
            {
                "type": "lyric",
                "text": "henstilling til innstilling om bistilling som"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "samskipnads-barnehavetante."
            },
            {
                "type": "lyric",
                "text": "Er det da virkelig rart det skranter?"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Mot Ottosens og Trosviks tyranni"
            },
            {
                "type": "lyric",
                "text": "og sparing fører vi en kamp."
            },
            {
                "type": "lyric",
                "text": "Betenk det, hvis du med et hovent blikk"
            },
            {
                "type": "lyric",
                "text": "og negativ kritikk sier «pamp» ."
            },
            {
                "type": "lyric",
                "text": "Ditt liv blir upåklag’lig"
            },
            {
                "type": "lyric",
                "text": "og daglig mer behag’lig."
            },
            {
                "type": "lyric",
                "text": "Din sorg den har vi klart å bøte på;"
            },
            {
                "type": "lyric",
                "text": "din velferd har vi daglig møte på!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "På skolen var jeg selvsagt alltid klassens tillitsgutt"
            },
            {
                "type": "lyric",
                "text": "og ble bestevenn med Rektor."
            },
            {
                "type": "lyric",
                "text": "Slik status får man bare ved å jobbe jevnt og trutt"
            },
            {
                "type": "lyric",
                "text": "og ved aldri å ytre et frekt ord."
            },
            {
                "type": "lyric",
                "text": "Ved å holde munn - kun tale det som måtte til"
            },
            {
                "type": "lyric",
                "text": "og være realistisk har jeg fått det til."
            },
            {
                "type": "lyric",
                "text": "Gjenvalg er lønnen for strevet;"
            },
            {
                "type": "lyric",
                "text": "å være med i utvalg er å leve."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Mitt mål det er å få en jobb en gang"
            },
            {
                "type": "lyric",
                "text": "i et departement. Ja, vent!"
            },
            {
                "type": "lyric",
                "text": "I Statens velsmurte byråkrati"
            },
            {
                "type": "lyric",
                "text": "kan sikkert jeg få bli konsulent."
            },
            {
                "type": "lyric",
                "text": "D’er tonn med dokumenter"
            },
            {
                "type": "lyric",
                "text": "som venter på talenter"
            },
            {
                "type": "lyric",
                "text": "og ungt, erfarent, driftig personell"
            },
            {
                "type": "lyric",
                "text": "med ubetinget tillit til seg selv!"
            }
        ]
    ]
}