    "contents": [
        [
            {
                "type": "lyric",
                "text": "Kjære herr Riksantikvar"
            },
            {
                "type": "lyric",
                "text": "Du vil bevare det vi har"
            },
            {
                "type": "lyric",
                "text": "Men det som Blindern er og var før"
            },
            {
                "type": "lyric",
                "text": "er mer enn en kjempedyr ytterdør."
            },
            {
                "type": "lyric",
                "text": "Blinderns sjel er mer enn bygningsmassen ,"
            },
            {
                "type": "lyric",
                "text": "selv om vi elsker det sure pianoet på dassen!"
            },
            {
                "type": "lyric",
                "text": " "
            },
            {
                "type": "chorus-header",
                "text": "Ref."
            },
            {
                "type": "chorus-lyric",
                "text": "Vi elsker Blindern, og alt hva Blindern er."
            },
            {
                "type": "chorus-lyric",
                "text": "Vi vil takke høye makter for at hjemmet vårt er her."
            },
            {
                "type": "chorus-lyric",
                "text": "For å redde stedet gjør vi alt i vår makt,"
            },
            {
                "type": "chorus-lyric",
                "text": "er fullt utrustet og klar for menneskejakt!"
            },
            {
                "type": "chorus-lyric",
                "text": "Blindern er ikke det det engang var,"
            },
            {
                "type": "chorus-lyric",
                "text": "Kjære far, hvorfor skapte du en Riksantikvar?"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Men tusen takk allikevel."
            },
            {
                "type": "lyric",
                "text": "For ellers måtte vi tatt farvel"
            },
            {
                "type": "lyric",
                "text": "med pærealléen vår;"
            },
            {
                "type": "lyric",
                "text": "kondisjonstrening gjennom mange år."
            },
            {
                "type": "lyric",
                "text": "Det gule bygg skal alltid bestå,"
            },
            {
                "type": "lyric",
                "text": "ekspropriasjon utgjør ingen fare nå!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Men ting er i ferd med å skje"
            },
            {
                "type": "lyric",
                "text": "for hjemmet vårt er slitt helt ned."
            },
            {
                "type": "lyric",
                "text": "På gule sal er det kjempestygge senger,"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "og vi kan ikke kjøpe nye når vi trenger."
            },
            {
                "type": "lyric",
                "text": "Det er dyrt å renovere alt."
            },
            {
                "type": "lyric",
                "text": "Dessverre har riksantikvaren allerede talt."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Husleien økes nok en gang."
            },
            {
                "type": "lyric",
                "text": "Vi kjenner en voksende trang"
            },
            {
                "type": "lyric",
                "text": "til å flytte i fra hjemmet vårt ,"
            },
            {
                "type": "lyric",
                "text": "selv om det føles veldig vondt og trist og sårt."
            },
            {
                "type": "lyric",
                "text": "Vi har ikke råd til mer nå…"
            },
            {
                "type": "lyric",
                "text": "Vi må på barrikadene stå!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Stedet rådes av anarki,"
            },
            {
                "type": "lyric",
                "text": "forsvunnet er vårt rang-hierarki."
            },
            {
                "type": "lyric",
                "text": "Borte vekk blir de stolte studenter"
            },
            {
                "type": "lyric",
                "text": "og pene, ikke fullt så smarte jenter."
            },
            {
                "type": "lyric",
                "text": "Igjen står vi der uten svar"
            },
            {
                "type": "lyric",
                "text": "og ser at helvete overtar!"
            }
        ]
    ]
}