    "contents": [
        [
            {
                "type": "lyric",
                "text": "No’n meter fra Vindern, ja akkurat her,"
            },
            {
                "type": "lyric",
                "text": "ligger et Studenterhjem blant eldgamle trær."
            },
            {
                "type": "lyric",
                "text": "Vakker er parken, men gult er vårt bygg."
            },
            {
                "type": "lyric",
                "text": "Vi synes fargen på veggen i spisesal’n er stygg!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Spisesal"
            },
            {
                "type": "lyric",
                "text": "Farger som plager deg"
            },
            {
                "type": "lyric",
                "text": "Spisesal"
            },
            {
                "type": "lyric",
                "text": "Alle forsyner seg"
            },
            {
                "type": "lyric",
                "text": "Spisesal"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Sprø er vi alle, det vet vi så godt"
            },
            {
                "type": "lyric",
                "text": "som flyttet inn her hvor de sa det var flott."
            },
            {
                "type": "lyric",
                "text": "Ja, gal må man være for å holde ut her"
            },
            {
                "type": "lyric",
                "text": "på dobbeltrom med en reaksjonær!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Reaksjonær"
            },
            {
                "type": "lyric",
                "text": "Det er jo alle her"
            },
            {
                "type": "lyric",
                "text": "Reaksjonær"
            },
            {
                "type": "lyric",
                "text": "Fra både fjern og nær"
            },
            {
                "type": "lyric",
                "text": "Reaksjonær"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Skulle du føle deg litt nedfor en dag,"
            },
            {
                "type": "lyric",
                "text": "sier fru FURU det er kun et bedrag."
            },
            {
                "type": "lyric",
                "text": "Og bråker du noe og griser litt til,"
            },
            {
                "type": "lyric",
                "text": "så rydder fru Jacobsen opp med et smil!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jacobsen og klor"
            },
            {
                "type": "lyric",
                "text": "– det lukter effektivt"
            },
            {
                "type": "lyric",
                "text": "Jacobsen og klor"
            },
            {
                "type": "lyric",
                "text": "Du slipper å vasse i dritt"
            },
            {
                "type": "lyric",
                "text": "Jacobsen og klor"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Litt praktisk arbeid gjør vi også på si’,"
            },
            {
                "type": "lyric",
                "text": "ja, dugnaden går vi til med stor energi."
            },
            {
                "type": "lyric",
                "text": "Vi venter nå bare på at vi får lov"
            },
            {
                "type": "lyric",
                "text": "å rive ned hele dritten i år."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Rive ned alt"
            },
            {
                "type": "lyric",
                "text": "det skulle vært gjort for lengst"
            },
            {
                "type": "lyric",
                "text": "Rive ned alt"
            },
            {
                "type": "lyric",
                "text": "det er kun det som trengs"
            },
            {
                "type": "lyric",
                "text": "Rive ned alt "
            }
        ]
    ]
}