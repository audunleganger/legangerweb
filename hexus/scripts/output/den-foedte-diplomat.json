    "contents": [
        [
            {
                "type": "lyric",
                "text": "Så er jeg da omsider blitt juridisk kandidat –"
            },
            {
                "type": "lyric",
                "text": "jeg er ikke nauden – jeg lo meg til lauden –"
            },
            {
                "type": "lyric",
                "text": "nu fedreland –står jeg parat."
            },
            {
                "type": "lyric",
                "text": "Mitt spesialfag var’ke lett; jeg valgte folkerett."
            },
            {
                "type": "lyric",
                "text": "Jeg trenger min viden – nå kaster jeg tweeden"
            },
            {
                "type": "lyric",
                "text": "–og bytter den med sjakett."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "For..."
            },
            {
                "type": "lyric",
                "text": "alle som kjenner meg –de sier at"
            },
            {
                "type": "lyric",
                "text": "jeg er som født til å bli diplomat."
            },
            {
                "type": "lyric",
                "text": "Jeg kan kunsten uten å bli tatt"
            },
            {
                "type": "lyric",
                "text": "kjøre bil grassat uten sertifikat."
            },
            {
                "type": "lyric",
                "text": "Kjent på byen som en fin gourmet –"
            },
            {
                "type": "lyric",
                "text": "franske viner er mitt metier –"
            },
            {
                "type": "lyric",
                "text": "innarbeidet er mitt renommé"
            },
            {
                "type": "lyric",
                "text": "som en attache in spe."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Jeg har et ekte motiv for å forme mitt liv."
            },
            {
                "type": "lyric",
                "text": "Tro ikke jeg er naiv -og konservativ."
            },
            {
                "type": "lyric",
                "text": "Jeg sa i tide adjø til Høyre-husets miljø –"
            },
            {
                "type": "lyric",
                "text": "for veien til suksess betinger små retretter"
            },
            {
                "type": "lyric",
                "text": "–pø om pø."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Og Morgenbladet har jeg -forlengst sagt opp –"
            },
            {
                "type": "lyric",
                "text": "det kan bli farlig for meg -så derfor STOPP!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Man er tross alt konsekvent – skal det bli resultat"
            },
            {
                "type": "lyric",
                "text": "av innbitt slit og streben for å bli diplomat."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Nå er veien ryddet klar til min aller første rang"
            },
            {
                "type": "lyric",
                "text": "til ry og heder og “gloire” i Utenriksdepartement."
            },
            {
                "type": "lyric",
                "text": "Gled deg, Halvard– her er jeg – jeg skal bli din "
            },
            {
                "type": "lyric",
                "text": "høyre hånd;"
            },
            {
                "type": "lyric",
                "text": "vi har begge en far og et navn sogar –"
            },
            {
                "type": "lyric",
                "text": "vi er oppdradd i den samme ånd!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Gled deg –Norge– her er jeg: Einar– jeg skal "
            },
            {
                "type": "lyric",
                "text": "motta deg"
            },
            {
                "type": "lyric",
                "text": "i noe annet enn et telt, når du kommer og "
            },
            {
                "type": "lyric",
                "text": "besøker meg."
            },
            {
                "type": "lyric",
                "text": "Så send meg ut til Washington, jeg inkarnerer "
            },
            {
                "type": "lyric",
                "text": "den etat"
            },
            {
                "type": "lyric",
                "text": "hvor en opportunist scorer stort til sist:"
            },
            {
                "type": "lyric",
                "text": "JEG ER DEN FØDTE DIPLOMAT!"
            }
        ]
    ]
}