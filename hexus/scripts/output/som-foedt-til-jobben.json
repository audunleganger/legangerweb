    "contents": [
        [
            {
                "type": "lyric",
                "text": "Tenk hvis Arne Scheie mistet jobben sin"
            },
            {
                "type": "lyric",
                "text": "Hvis det ikke var bruk for ham mer"
            },
            {
                "type": "lyric",
                "text": "Tenk hva som skjer hvis Arne Scheie mister jobben sin"
            },
            {
                "type": "lyric",
                "text": "For Arne Scheie er født til jobben sin"
            },
            {
                "type": "lyric",
                "text": "Skihopp og fotball er alt han kan"
            },
            {
                "type": "lyric",
                "text": "Han er en enkel mann, som kommentator er "
            },
            {
                "type": "lyric",
                "text": "han kjempefin"
            },
            {
                "type": "lyric",
                "text": "Men hva ville Arne Scheie ha gjort"
            },
            {
                "type": "lyric",
                "text": "hvis det ikke fantes sport? (Ikke stort!)"
            },
            {
                "type": "lyric",
                "text": "Da ville han gått på byen og"
            },
            {
                "type": "lyric",
                "text": "kommentert alle damer han så"
            },
            {
                "type": "lyric",
                "text": "da ville han kjørt en harry bil"
            },
            {
                "type": "lyric",
                "text": "og gitt pupper 19 og 20 i stil"
            },
            {
                "type": "lyric",
                "text": "han ville sagt, med leppa full av snus,"
            },
            {
                "type": "lyric",
                "text": "pupp er pupp og mus er mus – og han"
            },
            {
                "type": "lyric",
                "text": "ville vært så harry som det går an!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Så har vi Kjellbjørg Lunde som passer helt perfekt"
            },
            {
                "type": "lyric",
                "text": "som politiker i opposisjon."
            },
            {
                "type": "lyric",
                "text": "Hun er en grusom demon med sitt ansikt og stirreblikk."
            },
            {
                "type": "lyric",
                "text": "Tenk på hennes stemme og hennes dialekt."
            },
            {
                "type": "lyric",
                "text": "Det er faktisk et pluss å ikke være så pen"
            },
            {
                "type": "lyric",
                "text": "under Lunde-høringen, og Kjellbjørg Lunde er "
            },
            {
                "type": "lyric",
                "text": "jo helt unik."
            },
            {
                "type": "lyric",
                "text": "Men hva skulle hun ha funnet på"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "hvis det var hun som måtte gå "
            },
            {
                "type": "lyric",
                "text": "(Hva slags jobb kan hun få?)"
            },
            {
                "type": "lyric",
                "text": "Da ville hun sikkert jobba med"
            },
            {
                "type": "lyric",
                "text": "billett-kontroller for Sporveiene."
            },
            {
                "type": "lyric",
                "text": "Hvis du skulle ha glemt å stemple da"
            },
            {
                "type": "lyric",
                "text": "vil hun kreve at du må gå av"
            },
            {
                "type": "lyric",
                "text": "og ikke prøv å forklar deg vekk."
            },
            {
                "type": "lyric",
                "text": "Når Lunde griller slår du sprekk"
            },
            {
                "type": "lyric",
                "text": "Og hun ville skrevet ut bøter med smil om sin munn."
            },
            {
                "type": "lyric",
                "text": "(Jeg kunne ha betalt 450 kroner bare for å se henne"
            },
            {
                "type": "lyric",
                "text": "smile ett sekund!)"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Noen er som skapt til det de driver med"
            },
            {
                "type": "lyric",
                "text": "Frederic Hauge kun’ke jobba i NAF"
            },
            {
                "type": "lyric",
                "text": "og Tor Erling Staff ble advokat – ikke psykolog."
            },
            {
                "type": "lyric",
                "text": "Alle sa «Harald, du må bli konge», og jaggu ble han det."
            },
            {
                "type": "lyric",
                "text": "Han holder taler ved nyttår og elsker sitt land"
            },
            {
                "type": "lyric",
                "text": "som en alvorlig mann. Stillingen er fast, men dog"
            },
            {
                "type": "lyric",
                "text": "hva tror du at Harald gjør"
            },
            {
                "type": "lyric",
                "text": "hvis monarkiet dør (pga Se & Hør)."
            },
            {
                "type": "lyric",
                "text": "Da ville han sikkert spilt revy"
            },
            {
                "type": "lyric",
                "text": "og imitert en drita full Erik Bye."
            },
            {
                "type": "lyric",
                "text": "Da ville han holdt et folkelig show"
            },
            {
                "type": "lyric",
                "text": "med sitt sprudlende smil og vært grov."
            },
            {
                "type": "lyric",
                "text": "Og vinkedansen ville blitt suksess"
            },
            {
                "type": "lyric",
                "text": "med Sonja som sin kordame."
            },
            {
                "type": "lyric",
                "text": "Og det ville blitt den nye revykongen på ABC"
            },
            {
                "type": "lyric",
                "text": "han får sin egen komiserie på TV3."
            },
            {
                "type": "lyric",
                "text": "Kanskje greit han ikke begynner å jobbe med det!"
            }
        ]
    ]
}