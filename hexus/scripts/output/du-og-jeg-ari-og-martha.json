    "contents": [
        [
            {
                "type": "lyric",
                "text": "Han: Du og jeg har en himmel sammen."
            },
            {
                "type": "lyric",
                "text": "Hun: Du og jeg holder alltid liv i flammen"
            },
            {
                "type": "lyric",
                "text": "Begge: Amor og Venus vinker langs vår vei"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Han: Dine smil er bensin på bålet"
            },
            {
                "type": "lyric",
                "text": "Hun: Det er du som har fått meg til å stråle!"
            },
            {
                "type": "lyric",
                "text": "Begge: Solen står høyt, så lenge jeg har deg!"
            },
            {
                "type": "lyric",
                "text": "Nå er vi her, nå er vi sammen."
            },
            {
                "type": "lyric",
                "text": "Alle kan se oss, og alle glor!"
            },
            {
                "type": "lyric",
                "text": "Og alle kan høre når jeg hvisker ømt:"
            },
            {
                "type": "lyric",
                "text": "Han: Store ord"
            },
            {
                "type": "lyric",
                "text": "Hun: Store ord, store ord."
            }
        ],
        [
            {
                "type": "lyric",
                "text": "(avstanden øker mellom dem)"
            },
            {
                "type": "lyric",
                "text": "Han: Du og jeg deler evig lykke."
            },
            {
                "type": "lyric",
                "text": "Hun: Kjærlighet lar seg aldri undertrykke!"
            },
            {
                "type": "lyric",
                "text": "Begge: Bare et blikk fra deg gir alle svar."
            },
            {
                "type": "lyric",
                "text": "Han: Du og jeg (Hun: du og jeg...)"
            },
            {
                "type": "lyric",
                "text": "Du kan best forstå meg…"
            },
            {
                "type": "lyric",
                "text": "Du og jeg (Hun: du og jeg...)"
            },
            {
                "type": "lyric",
                "text": "Ikke syng i kjeften på meg!"
            },
            {
                "type": "lyric",
                "text": "Jeg er din helt..."
            },
            {
                "type": "lyric",
                "text": "Hun: ... og jeg din superstar!"
            }
        ],
        [
            {
                "type": "lyric",
                "text": "Begge: Natt blir til dag, drømmen får vinger"
            },
            {
                "type": "lyric",
                "text": "Hele min sjel åpenbarer seg."
            },
            {
                "type": "lyric",
                "text": "Det alle kan høre når jeg hvisker ømt."
            },
            {
                "type": "lyric",
                "text": "Det trengs ikke glitter og gods eller gull."
            },
            {
                "type": "lyric",
                "text": "Han: Bare deg"
            },
            {
                "type": "lyric",
                "text": "Hun: Bare meg"
            },
            {
                "type": "lyric",
                "text": "Begge: Bare meg. Bare vi to har - det vi to har."
            }
        ]
    ]
}